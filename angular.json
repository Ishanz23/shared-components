{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
    "packageManager": "pnpm",
    "analytics": false
	},
	"newProjectRoot": "projects",
	"projects": {
		"shared-components": {
			"projectType": "library",
			"root": "projects/shared-components",
			"sourceRoot": "projects/shared-components/src",
			"prefix": "shared",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/shared-components/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/shared-components/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/shared-components/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"tsConfig": "projects/shared-components/tsconfig.spec.json",
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						]
					}
				}
			}
		},
		"mfe-shared-components": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/mfe-shared-components",
			"sourceRoot": "projects/mfe-shared-components/src",
			"prefix": "mfe-shared",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/mfe-shared-components",
						"index": "projects/mfe-shared-components/src/index.html",
						"main": "projects/mfe-shared-components/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/mfe-shared-components/tsconfig.app.json",
						"assets": [
							"projects/mfe-shared-components/src/favicon.ico",
							"projects/mfe-shared-components/src/assets"
						],
						"styles": [
							"projects/mfe-shared-components/src/styles.css"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/mfe-shared-components/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/mfe-shared-components/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "mfe-shared-components:build:production",
							"extraWebpackConfig": "projects/mfe-shared-components/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "mfe-shared-components:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4500,
						"publicHost": "http://localhost:4500",
						"extraWebpackConfig": "projects/mfe-shared-components/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "mfe-shared-components:build",
						"extraWebpackConfig": "projects/mfe-shared-components/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/mfe-shared-components/tsconfig.spec.json",
						"assets": [
							"projects/mfe-shared-components/src/favicon.ico",
							"projects/mfe-shared-components/src/assets"
						],
						"styles": [
							"projects/mfe-shared-components/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
	}
}